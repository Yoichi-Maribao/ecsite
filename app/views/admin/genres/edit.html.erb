<div class="col">
  <h5>ジャンル編集</h5>
  <div class="row my-4">
    <div class="form-group">
      <%= form_with model: [:admin, @genre], local: true do |f| %>
        <%= f.label "ジャンル名" %>
        <%= f.text_field :name %>
        <%= f.submit "変更を保存", class:"btn btn-success" %>
      <% end %>
    </div>
  </div>
</div>